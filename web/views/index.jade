extends layout

block content
  p#copy
    strong
      | grow astrology like the woods
      | <br/>
      | grow stronger with each tree
  br
  p
    div(class= theme)

      h2#packages
        .line
          .l-room &nbsp;
          .s-room &nbsp;
          .l-room.mid.child
            a(href= "/to/precious") PRECIOUS
        .line
          .l-dep.mid.arrow
            span.adep &#x21D9;
          .r-dep.mid.arrow
            span.adep &#x21D8;
        .line
          .l-room.mid.child
            a(href= "/to/upon") UPON
          .s-room &nbsp;
          .o-room.mid.child
            a(href= "/to/sin") SIN
        .line.blank &nbsp;
        .line
          .m-room.child
            a(href= "/to/") ASTROLIN
        .line.blank &nbsp;

    #sunny
      h2#bottom.project.push
        - each cat in cats
          a.cat.front-cat(href="/cat/" + cat) #{cat}
        p#highlight.push
          a.cat.front-cat(href= "http://astrolet.co") astrolet.co
          //-a.cat.front-cat(href= "/data") have
          //-a.cat.front-cat(href= "https://github.com/astrolet/astrolin/wiki") wiki

